{
    "builds": [
      {
        "src": "frontend/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  